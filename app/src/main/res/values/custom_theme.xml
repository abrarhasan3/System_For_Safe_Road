<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyTextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <!-- border color -->
        <item name="boxStrokeColor">@color/mybeigeblue</item>

        <!-- The color of the label when it is collapsed and the text field is active -->
        <item name="hintTextColor">@color/mybeigeblue</item>
        <!-- The color of the label in all other text field states (such as resting and disabled) -->
        <item name="android:textColorHint">@color/mybeigeblue</item>

        <item name="colorOnSurface">@color/mybeigeblue</item>
        <item name="colorPrimary">@color/mybeigeblue</item>

    </style>
</resources>